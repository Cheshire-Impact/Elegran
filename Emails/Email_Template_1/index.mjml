<mjml>
  <mj-head>
    <mj-title>%%subject%%</mj-title>
    <!-- Set A Custom Webfont -->
    <mj-font name="Roboto" href="https://fonts.googleapis.com/css?family=Roboto:300,400&display=swap" />
    <mj-font name="Crimson Text" href="https://fonts.googleapis.com/css?family=Crimson+Text:600&display=swap" />
    <mj-attributes>

      <!-- Handle Pardot Regions -->
      <mj-section css-class="pardot-repeat-remove" background-color="#fff" />



      <!-- Set the Body and Default Section Background Colors. These Can Be Changed Using The Background Classes On An Individual Section-->
      <mj-body background-color="#f6f6f6" width="650px" />

      <!-- Default Styles for All Elements -->
      <mj-all color="#000" font-family="'Open Sans',Arial,Helvetica,sans-serif" font-size="14px" line-height="21px" />

      <mj-image align="center" fluid-on-mobile="true" href="#" css-class="pardot-region" />

    </mj-attributes>

    <mj-style>
      @import url('https://fonts.googleapis.com/css?family=Crimson Text:400,700|Roboto:300,400&display=swap');

      a {
        color: #000 !important;
        text-decoration: underline !important;
      }

      a:visited {
        color: #000 !important;
        text-decoration: underline !important;
      }

      a:link {
        color: #000 !important;
        text-decoration: underline !important;
      }


      a.greyLink {
        color: #333 !important;
        text-decoration: none !important;
      }

      a.greyLink:visited {
        color: #333 !important;
        text-decoration: none !important;
      }

      a.greyLink:link {
        color: #333 !important;
        text-decoration: none !important;
      }
    </mj-style>

    <mj-style>
      @media (max-width:480px) {
      td.mobileCenter > div {
      width: 100% !important;
      text-align: center !important;
      }
      .mobileHide{
      display:none;
      }
      }
    </mj-style>
  </mj-head>
